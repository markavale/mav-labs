# PACE AI - Environment Variables Template
# Copy this file to .env and fill in your values
# NEVER commit the .env file - it's in .gitignore

# =============================================================================
# LLM Provider (DeepSeek)
# =============================================================================
DEEPSEEK_API_KEY=sk-your-deepseek-key-here

# =============================================================================
# Research APIs
# =============================================================================
BRAVE_API_KEY=your-brave-api-key
SERPER_API_KEY=your-serper-api-key
APIFY_TOKEN=your-apify-token

# =============================================================================
# GitHub
# =============================================================================
GITHUB_TOKEN=ghp_your-github-token-here
GITHUB_OWNER=markavale

# =============================================================================
# Telegram
# =============================================================================
TELEGRAM_BOT_TOKEN=your-telegram-bot-token
MAV_TELEGRAM_ID=your-telegram-id

# =============================================================================
# Redis
# =============================================================================
REDIS_URL=redis://127.0.0.1:6379

# =============================================================================
# Dashboard Authentication (Supabase)
# Get from: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api
# Create a user: Supabase Dashboard > Authentication > Users > Add User
# =============================================================================
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
